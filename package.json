{
	"name": "lunchdev-community-calendar",
	"version": "1.0.0",
	"main": ".eleventy.js",
	"author": "Ben Myers <ben.myers@okstate.edu>",
	"license": "MIT",
	"scripts": {
		"build": "cross-env NODE_ENV=production eleventy",
		"dev": "cross-env NODE_ENV=development eleventy --serve",
		"format": "prettier --write \"src/**/*.md\"",
		"gen": "node ./cli",
		"recognize": "all-contributors add"
	},
	"dependencies": {
		"@11ty/eleventy": "^0.11.1",
		"calendar-link": "^2.0.8",
		"cross-env": "^7.0.3",
		"date-fns": "^2.16.1",
		"markdown-it": "^12.0.4",
		"markdown-it-emoji": "^2.0.0",
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@sherby/eleventy-plugin-files-minifier": "^1.1.1",
		"all-contributors-cli": "^6.19.0",
		"chalk": "^4.1.0",
		"commander": "^6.2.1",
		"husky": "^4.3.6",
		"inquirer": "^7.3.3",
		"node-emoji": "^1.10.0",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
